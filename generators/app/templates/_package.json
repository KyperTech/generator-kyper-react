{
  "name": "<%= appName %>",
  "version": "<%= version %>",
  "main": "bin/server.js",
  "scripts": {
    "build": "npm run clean && npm run build-bundle && npm run build-server && npm run build-copy",
    "build-bundle": "webpack -p --stats --progress --config webpack-production.config",
    "build-server": "webpack -p --stats --progress --config webpack-server-production.config",
    "build-copy": "cp -a assets build",
    "clean": "rimraf build",
    "start": "node bin/dev-server",
    "production": "node bin/server"
  },
  "author": "<%= githubUser %>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUser %>/<%= appName %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUser %>/<%= appName %>/issues"
  },
  "keywords": [
    "webpack",
    "redux",
    "react",
    "react-router",
    "kyper",
    "matter",
    "tessellate"
  ],
  "dependencies": {
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fireuser": "0.0.3",
    "history": "1.13.0",
    "lodash": "^4.11.1",
    "material-ui": "0.14.4",
    "proxy-middleware": "^0.15.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-redux": "4.4.5",
    "react-router": "2.0.0",
    "react-router-redux": "4.0.0-rc.1",
    "react-tap-event-plugin": "0.2.1",
    "redux": "3.2.1",
    "redux-thunk": "2.1.0",
    "serve-static": "1.10.2",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "node-sass": "^3.3.2",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.12.3",
    "transform-loader": "^0.2.3",
    "webpack-dev-server": "^1.14.1"
  }
}
